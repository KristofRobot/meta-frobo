##############################
#	7-port powered USB hub
#	
#               -----
#       port 6 -     - port 1
#       port 7 -  H  - port 2
#          LED -  U  - port 3 
#        USB B -  B  - port 4
#     Power 5V -     - port 5
#		-----
##############################

#Port 1
SUBSYSTEMS=="usb", KERNEL=="ttyUSB[0-9]*", KERNELS=="3-1.1.4:1.0", SYMLINK+="arduino_imu"
#Port 2
#USB speaker - no udev rule necessary - autodetected
#Port 3
SUBSYSTEMS=="usb", KERNEL=="ttyACM[0-9]*", KERNELS=="3-1.3:1.0", SYMLINK+="arduino_motor"
#Port 4
#USB microphone - no udev rule necessary - autodetected
#Port 5
SUBSYSTEMS=="usb", KERNEL=="ttyUSB[0-9]*", KERNELS=="3-1.2:1.0", SYMLINK+="arduino_sonar"
#Port 6
SUBSYSTEMS=="usb", KERNEL=="ttyUSB[0-9]*", KERNELS=="3-1.1.1:1.0", SYMLINK+="lidar"
#Port 7 
#Kinect - no udev rule necessary - autodetected
